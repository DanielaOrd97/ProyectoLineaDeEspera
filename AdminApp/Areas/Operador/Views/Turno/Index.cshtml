@model AdminApp.Models.ViewModels.TurnoViewModel1


<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/css/Style.css" />
    <link rel="stylesheet"
          href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css" />
    <title>Cajero</title>
</head>
<body class="BodyCajero">
    <nav class="navbar-horizontal">
        <h1 class="navbar-logo">BancoTec</h1>
        <h1 class="navbar-username"><i class="fas fa-user"></i>@Model.NombreCaja</h1>
        <div class="navbar-logout">
            <a href="#logout"><i class="fas fa-sign-out-alt"></i>Cerrar Sesión</a>
        </div>
    </nav>
    <div class="container">
        <div class="turno">
            <p class="label">Turno:</p>
            <p class="codigo"><i id="user" class="fas fa-user"></i>@Model.IdTurno</p>
        </div>
        <div class="botones">
            <button class="btnSaltar">Saltar</button>
            <a style="text-decoration: none" class="btnAnterior" href="~/Operador/Turno/AtrasarTurno">Anterior</a>
            <a style="text-decoration: none" class="btnSiguiente" href="~/Operador/Turno/LlamarCliente/@Model.IdTurno">Llamar</a>
            <a style="text-decoration: none" class="btnSiguiente" href="#">Atender</a>
            <a style="text-decoration: none" class="btnSiguiente" href="~/Operador/Turno/AdelantarTurno">Siguiente</a>
        </div>
    </div>
</body>
</html>
